@model IEnumerable<TuVanKhamBenh.Models.LichKham>

@{
  ViewBag.Title = "LSDL";
  Layout = "~/Views/Shared/_LayoutUser.cshtml";
}

<p style="font-size:15px; font-family:Tahoma; margin-left:250px">Trang chủ > Lịch sử đặt lịch khám</p>
<div class="List">
  @if (Model.Count() > 0)
  {
    <table class="user-table">
      <tr>
        <th>
          @Html.DisplayNameFor(model => model.Info)
        </th>
        <th>
          @Html.DisplayNameFor(model => model.Username)
        </th>
        <th>
          @Html.DisplayNameFor(model => model.Ten)
        </th>
        <th>
          @Html.DisplayNameFor(model => model.NgayDat)
        </th>
        <th>
          @Html.DisplayNameFor(model => model.NgayHen)
        </th>
      </tr>

      @foreach (var item in Model)
      {
        <tr>
          <td>
            @Html.DisplayFor(modelItem => item.Info)
          </td>
          <td>
            @Html.DisplayFor(modelItem => item.Username)
          </td>
          <td>
            @Html.DisplayFor(modelItem => item.Ten)
          </td>
          <td>
            @Html.DisplayFor(modelItem => item.NgayDat)
          </td>
          <td>
            @Html.DisplayFor(modelItem => item.NgayHen)
          </td>
        </tr>
      }

    </table>
  }
  else
  {
        <p style = "font-size: 18px;font-family:Tahoma; padding: 50px;" > Danh sách trống! </p>
   }
  </div>
